{
  "ios": [
    {
      "app_name": "微信",
      "bundle_id": "com.tencent.xin",
      "scheme": "weixin://",
      "description": "微信官方 URL Scheme",
      "parameters": [
        {
          "param": "dl/",
          "description": "打开指定页面"
        },
        {
          "param": "dl/moments",
          "description": "打开朋友圈"
        }
      ],
      "example": "weixin://dl/moments"
    },
    {
      "app_name": "支付宝",
      "bundle_id": "com.alipay.iphoneclient",
      "scheme": "alipay://",
      "description": "支付宝官方 URL Scheme",
      "parameters": [
        {
          "param": "platformapi/startapp",
          "description": "启动应用"
        },
        {
          "param": "pay",
          "description": "支付页面"
        }
      ],
      "example": "alipay://platformapi/startapp"
    },
    {
      "app_name": "淘宝",
      "bundle_id": "com.taobao.taobao",
      "scheme": "taobao://",
      "description": "淘宝官方 URL Scheme",
      "parameters": [
        {
          "param": "item?id=",
          "description": "查看商品"
        },
        {
          "param": "search?q=",
          "description": "搜索商品"
        }
      ],
      "example": "taobao://item?id=123456789"
