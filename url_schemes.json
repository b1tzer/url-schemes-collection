        },
        {
          "param": "profile/",
          "description": "查看个人资料"
        }
      ],
      "example": "mqq://im/chat?chat_type=wpa&uin=12345678"
    },
    {
      "app_name": "抖音",
      "bundle_id": "com.ss.iphone.ugc.Aweme",
      "scheme": "douyin://",
